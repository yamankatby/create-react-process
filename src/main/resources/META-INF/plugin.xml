<idea-plugin>
    <id>com.swazerlab.create-react-process.create-react-process</id>
    <name>Create React Process</name>
    <vendor email="m.yaman.katby@gmail.com" url="https://yaman.idewaxa.com/">Swazer LAB.</vendor>

    <description><![CDATA[
    Make your development process faster by creating processes by one tab.
    ]]></description>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <group id="MyPlugin.SampleMenu" text="React" description="Greeting menu">
            <add-to-group group-id="MainMenu" anchor="last"/>
            <action id="com.swazerlab.create-react-process" class="CreateProcessAction" text="Create New Process"
                    description="Create new React process"/>
            <action id="com.swazerlab.create-react-action" class="CreateActionAction" text="Create New Action"
                    description="Create new React action"/>
        </group>
    </actions>
</idea-plugin>
