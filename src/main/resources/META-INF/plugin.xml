<idea-plugin>
    <id>com.swazerlab.create-react-process.create-react-process</id>
    <name>Plugin display name here</name>
    <vendor email="support@yourcompany.com" url="http://www.yourcompany.com">YourCompany</vendor>

    <description><![CDATA[
    Enter short description for your plugin here.<br>
    <em>most HTML tags may be used</em>
    ]]></description>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <group id="MyPlugin.SampleMenu" text="React" description="Greeting menu">
            <add-to-group group-id="MainMenu" anchor="last"/>
            <action id="com.swazerlab.create-react-process" class="CreateProcessAction" text="Create New Process"
                    description="Create new React process"/>
            <action id="com.swazerlab.create-react-action" class="CreateActionAction" text="Create New Action"
                    description="Create new React action"/>
        </group>
    </actions>
</idea-plugin>
